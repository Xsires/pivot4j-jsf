<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
	name="Pivot4J JSF Sample">
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.0</version>
	</skin>

	<body>
		<links>
			<item name="GitHub Project Page" href="https://github.com/mysticfall/pivot4j-jsf"/>
			<item name="Download" href="http://github.com/mysticfall/pivot4j-jsf/downloads"/>
			<item name="Support Forum" href="http://groups.google.com/d/forum/pivot4j-list"/>
			<item name="Pivot4J Project" href="http://mysticfall.github.com/pivot4j"/>
		</links>
		<menu name="Overview">
			<item name="Introduction" href="index.html" />
		</menu>
		<menu ref="reports" />
	</body>

	<custom>
		<fluidoSkin>
			<sideBarEnabled>true</sideBarEnabled>
			<sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
			<profile>production</profile>
			<gitHub>
				<projectId>mysticfall/pivot4j-jsf</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>black</ribbonColor>
			</gitHub>
			<twitter>
				<user>pivot4j</user>
				<showUser>true</showUser>
				<showFollowers>true</showFollowers>
				</twitter>
			<googleSearch />
		</fluidoSkin>
	</custom>
</project>
